{
  "extends": "../tsconfig.json",
  "include": [
    "./**/*.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "types": [
      "cypress",
      "@percy/cypress"
    ],
    "lib": [
      "es2015",
      "dom"
    ],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true,
    "noImplicitAny": false // make typescript tolerate parameters without declaring their type
  }
}
